{
  "compilerOptions": {
      "strict": true,
      "target": "es6",
      "lib": ["ESNext", "DOM", "WebWorker"],
      "declaration": true,
      "outDir": "./es",
      "baseUrl": "./",
      "allowJs": true,
      "jsx": "react",
      "moduleResolution": "node",
      "isolatedModules": true,
      "esModuleInterop": true,
      "skipLibCheck": true
  },
  "include": ["src/**/*", "./typings.d.ts"]
}